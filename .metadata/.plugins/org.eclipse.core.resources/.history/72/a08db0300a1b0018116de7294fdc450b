                                                                  if((result > 12)||(result < 1))  {if (true) throw new Error("(Line:"+jj_input_stream.line+",Column:"+jj_input_stream.column+"): Wrong month format.");}
                                                                  {if (true) return result;}
    throw new Error("Missing return statement in function");
